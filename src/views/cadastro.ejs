
<%- include('partials/head')%> 
<%- include('partials/header')%> 
<link rel="stylesheet" href="/css/cadastro.css">

<body>
<h2 class="titulo">CADASTRO</h2>

<form action="/cadastro" method="POST">
        
        <div class="input-field"></div>
            <input type="text" name="Nome" id="username" placeholder="Nome Completo">
            <div class="underline"></div>
        </div>
       
        <div class="input-field"></div>
            <input type="text" name="email" id="email" placeholder="email">
            <div class="underline"></div>
        </div>
        <div class="input-field"></div>
            <input type="text" name="endereço" id="endereço" placeholder="endereço">
            <div class="underline"></div>
        </div>
        <div class="input-field"></div>
            <input type="text" name="endereço" id="telefone" placeholder="telefone">
            <div class="underline"></div>
        </div>

        <div class="input-field"></div>
            <input type="password" name="password" id="password" placeholder="Sua Senha">
            <div class="underline"></div>
        </div>

        <button class="botao" type="submit" onsubmit="">CADASTRAR</button>

    </form>
    
        <div class="cadastro">
            <a href="http://localhost:3000/cadastro">entrar <i class="fas fa-user-check"></i></a>
        </div>
    </div>
</body> 
